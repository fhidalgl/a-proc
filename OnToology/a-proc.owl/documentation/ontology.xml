    <!-- http://purl.org/a-proc#ActoAdministrativo -->


    <owl:Class rdf:about="http://purl.org/a-proc#ActoAdministrativo">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/p-plan#Step"/>
        <rdfs:comment xml:lang="en">An administrative act is any unilateral declaration of will performed in the exercise of the administrative function that produces individual legal effects immediately</rdfs:comment>
        <rdfs:comment xml:lang="es">Un acto administrativo es toda declaración unilateral de voluntad realizada en el ejercicio de la función administrativa que produce efectos jurídicos individuales de forma inmediata</rdfs:comment>
        <rdfs:label xml:lang="es">ActoAdministrativo</rdfs:label>
        <ns:term_status xml:lang="en">stable</ns:term_status>
    </owl:Class>
    


    <!-- http://purl.org/a-proc#ActoExpediente -->


    <owl:Class rdf:about="http://purl.org/a-proc#ActoExpediente">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/p-plan#Entity"/>
        <rdfs:comment xml:lang="es">Cada uno de las actuaciones administrativas realizadas en una fase de un expediente. Se corresponde con un ActoAdministrativo.</rdfs:comment>
        <rdfs:comment xml:lang="en">Each of the administrative actions carried out in one phase of an administrative file.</rdfs:comment>
        <rdfs:label xml:lang="es">ActoExpediente</rdfs:label>
        <ns:term_status xml:lang="en">stable</ns:term_status>
    </owl:Class>
    


    <!-- http://purl.org/a-proc#ActoOrdenacion -->


    <owl:Class rdf:about="http://purl.org/a-proc#ActoOrdenacion">
        <rdfs:subClassOf rdf:resource="http://purl.org/a-proc#ActoAdministrativo"/>
        <rdfs:comment xml:lang="en">It is considered an act of ordination that act that can be instantiated at any time of the processing of a file.</rdfs:comment>
        <rdfs:comment xml:lang="es">Se considera un acto de ordenación aquel acto que puede ser instanciado en cualquier momento de la tramitación de un expediente.</rdfs:comment>
        <rdfs:label xml:lang="es">ActoOrdenacion</rdfs:label>
        <ns:term_status xml:lang="en">stable</ns:term_status>
    </owl:Class>
    


    <!-- http://purl.org/a-proc#AportarDocumentacion -->


